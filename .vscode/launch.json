{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug VulHunter",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/VulHunter/main/main.py",
            "args": [
                "--contract", "VulHunter/contracts_test/thedao.sol",
                // "--solc-version", "0.8.10",
                "--filetype", "solidity",
                "--model-dir", "VulHunter/models",
                "--instance-len", "10",
                "--detectors", "reentrancy-eth"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Debug Instance Generation",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/VulHunter/main/main.py",
            "args": [
                "--train-contracts", "Dataset_CGT/cgt",
                "--train-solcversions", "Dataset_CGT/ds_solc_versions.json",
                "--instance-dir", "input"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}"
        }
    ]
}